# Managed by Symfony
/var/*
!var/SymfonyRequirements.php

# Cache / Logs / Sessions (Symfony3)
!/var/cache
/var/cache/*
!var/cache/.gitkeep
!/var/logs
/var/logs/*
!var/logs/.gitkeep
!/var/sessions
/var/sessions/*
!var/sessions/.gitkeep

# Parameters
/app/config/parameters.yml

# Managed by Composer
/var/bootstrap.php.cache
/bin/*
!bin/console
!bin/symfony_requirements

# Symfony dependencies
/vendor/

# Assets and user uploads
/web/bundles/

# PHPUnit
/phpunit.xml

# Build data
/build/
!/web/dist
/web/dist/*

# Composer PHAR
/composer.phar

# Managed by Node
!/node_modules
/node_modules/*
